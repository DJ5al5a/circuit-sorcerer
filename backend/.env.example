# Backend Configuration Example
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# PostgreSQL Database
DB_HOST=your-database-host
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# Discord Webhooks
DISCORD_MESSAGES_WEBHOOK=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN
DISCORD_REQUESTS_WEBHOOK=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN

# Telegram Bot
TELEGRAM_BOT_TOKEN=YOUR_BOT_TOKEN_FROM_BOTFATHER
TELEGRAM_CHAT_ID=YOUR_TELEGRAM_USER_ID

# Emby Server
EMBY_SERVER_URL=https://your-emby-server.example.com
EMBY_API_KEY=your_emby_admin_api_key
EMBY_LOCAL_IP=192.168.x.x

# n8n Instance
N8N_URL=https://your-n8n-instance.com
